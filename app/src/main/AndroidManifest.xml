<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nhsc">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.GetStarted"
            android:screenOrientation="portrait" />
        <activity
            android:name=".login.activities.Sign_in"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registration.activities.Register1"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registration.activities.Register2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registration.activities.Register3"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activities.HomeScreen"
            android:screenOrientation="portrait" />

        <activity
            android:name=".Activities.Forgot_password"
            android:screenOrientation="portrait" />

        <activity
            android:name=".Activities.Category_Tabs"
            android:screenOrientation="portrait" />

        <activity
            android:name=".home.activities.Home_Screen"
            android:label="@string/title_activity_home__screen"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>